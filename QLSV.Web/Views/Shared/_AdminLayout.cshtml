<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Admin - Dashboard</title>

    <!-- Custom fonts for this template-->
    <link href="~/custom/admin/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css" integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <!-- Custom styles for this template-->
    <link href="~/custom/admin/css/sb-admin-2.min.css" rel="stylesheet">
    <link href="~/custom/admin/css/customer.css" rel="stylesheet" />
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
    <link rel="stylesheet" href="//cdn.datatables.net/1.11.2/css/jquery.dataTables.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.css" />
    <script src="https://kit.fontawesome.com/710495fa1f.js" crossorigin="anonymous"></script>
</head>
<body>
    <div id="wrapper">
        <!-- Sidebar -->
        <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion" id="accordionSidebar">
            <!-- Nav Item - Dashboard -->
            <li class="nav-item active">
                <a class="nav-link" href="#">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Dashboard</span>
                </a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider" />

            <!-- Nav Item - Department -->
            <li class="nav-item">
                <a class="nav-link" href="/Admin/Departments/Index">
                    <i class="fa-solid fa-book"></i>
                    <span>Danh sách khoa</span>
                </a>
            </li>

            <!-- Nav Item - Teacher -->
            <li class="nav-item">
                <a class="nav-link" href="/Admin/Teachers/Index">
                    <i class="fa-solid fa-book"></i>
                    <span>Danh sách giáo viên</span>
                </a>
            </li>

            <!-- Nav Item - Course -->
            <li class="nav-item">
                <a class="nav-link" href="/Admin/Courses/Index">
                    <i class="fa-solid fa-book"></i>
                    <span>Danh sách môn học</span>
                </a>
            </li>

            <!-- Nav Item - Student -->
            <li class="nav-item">
                <a class="nav-link" href="/Admin/Students/Index">
                    <i class="fa-solid fa-book"></i>
                    <span>Danh sách sinh viên</span>
                </a>
            </li>

            <!-- Nav Item - Classroom -->
            <li class="nav-item">
                <a class="nav-link" href="/Admin/Classrooms/Index">
                    <i class="fa-solid fa-book"></i>
                    <span>Danh sách lớp học phần</span>
                </a>
            </li>

            <!-- Nav Item - PrimaryClass -->
            <li class="nav-item">
                <a class="nav-link" href="/Admin/PrimaryClasses/Index">
                    <i class="fa-solid fa-book"></i>
                    <span>Danh sách lớp</span>
                </a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider" />
            <!-- Heading -->
            <div class="sidebar-heading">
                Users
            </div>
            <!-- Nav Item - Roles -->
            <li class="nav-item">
                <a class="nav-link" href="#">
                    <i class="fa-solid fa-book"></i>
                    <span>Quản lý quyền</span>
                </a>
            </li>
        </ul>
        <!-- End of Sidebar -->
        <!--Page content-->
        <div id="content-wapper" class="container-fluid d-flex flex-column">
            <!--Main content-->
            <div id="content">
                <!--Topbar-->
                <partial name="~/Views/Shared/_NavbarPartial.cshtml" />
                <!--End of Topbar-->
                <div class="container-fluid">
                    <main role="main" class="pb-3" style="overflow-x: hidden">
                        @RenderBody()
                    </main>
                </div>
            </div>
            <!--End of Main content-->

        </div>
    </div>
    
    <!--Script file-->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
